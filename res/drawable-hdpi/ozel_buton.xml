<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#FEFBFA" />
      <stroke
          android:width="1dp"
          android:color="#C7C7C7" />
      <corners
          android:radius="10dp" />
      <padding
          android:left="1dp"
          android:top="1dp"
          android:right="1dp"
          android:bottom="1dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#FEFEFE"
          android:endColor="#DADAD9"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#ABAAAA" />
      <corners
          android:radius="10dp" />
      <padding
          android:left="1dp"
          android:top="1dp"
          android:right="1dp"
          android:bottom="1dp" />
    </shape>
  </item>
</selector>